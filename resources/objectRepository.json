{
    "homePage":
    {
      "homePageHeaderText": ["xpath", "//h1[contains(text(),'Create a Website You’re Proud Of')]"],
      "homePageLogoLink": ["xpath", "//a[@href='https://www.wix.com'"],
      "signInLink": ["xpath", "//a[contains(text(), 'Sign In')]"],
      "signUpLink": ["css", "[ng-click='$ctrl.onClickSignup()']"],
      "heading": ["xpath","//h1[@class='ng-binding']"],
      "toDoTextBox": ["model","todoList.todoText"],
      "addButton": ["xpath","//input[@value='add']"],
      "learnLink": ["xpath", "//a[contains(text(),'Learn')]"],
      "checkBoxLabel": ["xpath","//form[@ng-submit='todoList.addTodo()']/parent::div/child::ul/li[last()]/label/span"]
      
    },

    "loginPage": {
      "header": ["xpath", "//h1[contains(text(), 'Log In')]"],
      "email": ["name", "email"],
      "password": ["name", "password"],
      "loginButton":["name", "submit"]
    },
    "signUpPage": {
      "header": ["xpath", "//h1[contains(text(), 'Sign Up')]"],
      "email": ["name", "email"],
      "emailConfirm": ["name", "emailConfirm"],
      "password": ["name", "password"],
      "confirmPassword": ["name","passwordConfirm"],
      "submitButton":["name", "submit"],
      "facebookSignUp": ["xpath", "//span[contains(text(), 'Continue with Facebook')]"],
      "googleSignUp": ["xpath", "//span[contains(text(), 'Continue with Google')]"],
      "captcha": ["xpath", "//div[@class='recaptcha-checkbox-border']"],
      "loginLink":["xpath", "//a[contains(text(), 'Log In')]"]
    },
    "introPage": {
      "getStartedMsg": ["xpath", "//span[contains(text(), 'Let’s get started')]"]
    }
  }